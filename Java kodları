public class Main{
public static void main(String[] args){

    int a=0; //while için
    int b=1; // do while için

    while (a<10){
        a++;
        System.out.println(a);
    }

    for (int i=1;i<11;i++){
        System.out.println(i);
    }
    
    do{
        System.out.println(b);
        b++;
    }while (b<11);

}
}
